{
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "editor.autoIndent": "keep",
    "editor.detectIndentation": false,
    "editor.rulers": [
        100
    ],
    "[restructuredtext]": {
        "editor.rulers": [
            80
        ]
    },
    "[python]": {
        "editor.rulers": [
            80
        ]
    },
    "[cython]": {
        "editor.rulers": [
            80
        ]
    },
    "[markdown]": {
        "editor.rulers": [
